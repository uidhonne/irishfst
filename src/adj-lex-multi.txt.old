!ADJECTIVES QUALIFYING FEM SINGULAR NOUNS


LEXICON Fem_nom_voc_sg
! adj following a fem. noun is always lenited (^Sé) regardless of whether 
! the preceding noun is lenited or eclipsed (neither ??)
! as is the vocative after vocative particle "a"

+Sg:^Sé		#;

! SAME FORM IS USED FOR COMPARATIVE AND FEM GEN SG

LEXICON Fem_gen-D2A_sg			! like Noun Declension 2
+Fem+Gen+Sg:^Caole	#;
+Comp:^Caole	#;
!!+Comp+Len:^Caole^Sé	#;
+Comp+Len:^Caole^Sé	#;


LEXICON Fem_gen-D2B_sg			! like Noun Declension 2 but without "e" appended
+Fem+Gen+Sg:^Ath	#;
+Comp:^Ath	#;
!!+Comp+Len:^Ath^Sé	#;
+Comp+Len:^Ath^Sé	#;


LEXICON Fem_gen-D2C_sg			! a new change
+Fem+Gen+Sg:^Aththí	#;
+Comp:^Aththí	#;
!!+Comp+Len:^Aththí^Sé	#;
+Comp+Len:^Aththí^Sé	#;


LEXICON Fem_gen-D2D_sg			! a new change
+Fem+Gen+Sg:^Aththaí	#;
+Comp:^Aththaí	#;
!!+Comp+Len:^Aththaí^Sé	#;
+Comp+Len:^Aththaí^Sé	#;

LEXICON Fem_gen-D3_sg			! like Noun Declension 3
+Fem+Gen+Sg:^Leaa	#;
+Comp:^Leaa	#;
!!+Comp+Len:^Leaa^Sé	#;
+Comp+Len:^Leaa^Sé	#;

LEXICON Fem_gen-D4_sg			! like Noun Declension 4 (no change)
+Fem+Gen+Sg:0	#;
+Comp:0	#;
!!+Comp+Len:^Sé	#;
+Comp+Len:^Sé	#;



!ADJECTIVES QUALIFYING MASC SINGULARNOUNS

LEXICON Masc_nom_sg
! adj following a masc. noun is lenited only if the noun is lenited

! Len tags changed 27/5/2003 EUíD
!+Sg+LenYES:^Sé	#;				! ar an fhear "bheag" - on the small man
!+Sg+LenNO:0 	#;				! BUT ar an bhfear "beag" - on the small man

!!+Sg+Len:^Sé	#;				! Lenite if noun is lenited e.g. ar an fhear "bheag" - on the small man
+Sg:^Sé	#;				! Lenite if noun is lenited e.g. ar an fhear "bheag" - on the small man
+Sg:0 		#;				! Otherwise no lenition e.g. ar an bhfear "beag" - on the small man


LEXICON Masc_gen-D4_voc_sg		! like Noun Declension 4
	
+Sg:^Sé		#;				! hata an fhir bhig - the small man's hat
						! a fhir bhig - O small man

